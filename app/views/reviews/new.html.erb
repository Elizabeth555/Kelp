<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <%= form_for :review, url: reviews_path do |f| %>
      <p>
        <%= f.label :rating %><br>
        <%= f.number_field :rating %>
      </p>

      <p>
        <%= f.label :comment %><br>
        <%= f.text_field :comment %>
      </p>

      <p>
        <%= f.submit %>
      </p>

    <%end%>


  </body>
</html>
